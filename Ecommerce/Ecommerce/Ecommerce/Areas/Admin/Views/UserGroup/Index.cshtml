@model List<Model.EF.UserGroup>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layouts.cshtml";
}

@section header{Loại tài khoản}
@section jsFooter{}

<div class="row">
    <div class="col-12">
        <div class="card-box">
            <form class="form-horizontal">
                @foreach (var item in Model)
                {
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="simpleinput">@item.ID</label>
                        <div class="col-sm-10">
                            @item.Name
                        </div>
                    </div>
                }
            </form>
        </div>
    </div>
</div>
